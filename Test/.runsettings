<?xml version="1.0" encoding="utf-8" ?> 

<RunSettings>
	<NUnit>
		<MaxCpuCount>4</MaxCpuCount>
		<NumberOfTestWorkers>2</NumberOfTestWorkers>
		<TestOutputXml>Results</TestOutputXml>
		<DisplayName>FullName</DisplayName>
		<UseTestNameInConsoleOutput>true</UseTestNameInConsoleOutput>
		<ConsoleOut>1</ConsoleOut>
		<Verbosity>3</Verbosity>
	</NUnit>
	
	<RunConfiguration>
	    <ResultsDirectory>bin/Debug/results</ResultsDirectory>
	</RunConfiguration>
	
	<LoggerRunSettings>
            <Loggers>
                <Logger friendlyName="console" enabled="True">
                    <Configuration>
                        <Verbosity>detailed</Verbosity>
                    </Configuration>
                </Logger>
                <Logger friendlyName="trx" enabled="True">
                    <Configuration>
                        <LogFileName>TestResults.trx</LogFileName>
                    </Configuration>
                </Logger>
                <Logger friendlyName="html" enabled="True">
                    <Configuration>
                        <LogFileName>TestResults.html</LogFileName>
                    </Configuration>
                </Logger>
            </Loggers>
        </LoggerRunSettings>
</RunSettings>